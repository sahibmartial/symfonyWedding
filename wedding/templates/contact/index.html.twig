{% extends 'base.html.twig' %}

{% block title %}TEAM_MAYA-Contact{% endblock %}

{% block content %}

	{% if notification %}
	<div class="alert alert-info">
		{{ notification }}
		<hr>
	</div>
	{% endif %}
	<h1>Nous joindre</h1>

	{{form_start(form)}}
	<div class="row">
	<div class="col-md-6">{{ form_row(form.nom) }}</div>
	<div class="col-md-6">{{ form_row(form.prenom) }}</div>
	<div class="col-md-6">{{form_row(form.email)}}</div>
	<div class="col-md-6">{{form_row(form.content)}}</div>
   </div>

	{{form_end(form)}}

{% endblock %}
